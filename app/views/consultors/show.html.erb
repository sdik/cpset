<p id="notice"><%= notice %></p>
<div class="page-header">
  <h1>consultor</h1>
</div>

<dl class="dl-horizontal">
  <dt><strong>Nome:</strong></dt>
  <dd><%= @consultor.nome %></dd>
  <dt><strong>Telefone:</strong></dt>
  <dd><%= @consultor.telefone %></dd>
  <dt><strong>Email:</strong></dt>
  <dd><%= @consultor.email %></dd>
</dl>

<div class="form-actions">
  <%= link_to 'Voltar', consultors_path, :class=>"btn" %>
  <%= link_to 'Editar', edit_consultor_path(@consultor),:class => "btn" %> 
  <%= link_to 'Apagar', @consultor, confirm: 'Desejas Apagar?',method: :delete ,:class => 'btn btn-danger'%>
</div>